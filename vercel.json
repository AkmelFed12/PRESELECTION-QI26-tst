{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs24.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/quiz-2025-media/(.*)",
      "dest": "/public/quiz-2025-media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
